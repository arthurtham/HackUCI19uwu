            <string>if not instance_exists(obj_cookie_good) {

    //wipeout phase
    with obj_cookie_bad instance_destroy()
    with obj_boba instance_destroy()
    with obj_cookie_neutral instance_destroy()
    

    //new phase
    ++stage;
    var _depth = 0;
    
    if (category_max &lt; array_length_1d(category)-1) and (stage mod 3 == 0) {
        ++category_max;
    }
    
    if stage &gt; 4 {
        with obj_sponsors {
            if visible != 1 visible = 1;
            image_index = irandom_range(0,image_number-1)//other.stage mod image_number
            if !bg bg = true;
        }
        bgcolor = bgcolors[irandom_range(0,array_length_1d(bgcolors)-1)]
    }
    
    if stage &gt; 6 {
        repeat(stage / 3) {
            inst = instance_create(irandom_range(room_width/8,room_width/8*7),
                        irandom_range(room_height/8,room_height/8*6),
                        obj_cookie_neutral);
            inst.depth = _depth--;
            //inst.sprite_index = category[irandom_range(0,category_max)]+1;
            //inst.spin = irandom_range(0,1);
        }
    }
    
    if stage &gt; 7 {
        repeat(stage / 4) {
            inst = instance_create(irandom_range(room_width/8,room_width/8*7),
                        irandom_range(room_height/8,room_height/8*6),
                        obj_boba);
            inst.depth = _depth--;
            //inst.sprite_index = category[irandom_range(0,category_max)]+1;
            //inst.spin = irandom_range(0,1);
        }
    }
    
    if stage &gt; 9 {
        repeat(stage / 5) {
            inst = instance_create(irandom_range(room_width/8,room_width/8*7),
                        irandom_range(room_height/8,room_height/8*6),
                        obj_banhmi);
            inst.depth = _depth--;
            //inst.sprite_index = category[irandom_range(0,category_max)]+1;
            //inst.spin = irandom_range(0,1);
        }
    }
    
    if stage &gt; 8 {
        repeat(stage / 3) {
            inst = instance_create(irandom_range(room_width/8,room_width/8*7),
                        irandom_range(room_height/8,room_height/8*6),
                        obj_cookie_bad);
            inst.depth = _depth--;
            inst.sprite_index = category[irandom_range(0,category_max)]+1;
            inst.spin = irandom_range(0,1);
        }
    
    }
    
    if stage == 10 {
        inst = instance_create(irandom_range(room_width/8,room_width/8*7),
                        room_height/8*6,
                        obj_shuttle);
    }
    
    repeat(min(stage,15)) {
        inst = instance_create(irandom_range(room_width/8,room_width/8*7),
                        irandom_range(room_height/8,room_height/8*6),
                        obj_cookie_good);
        inst.depth = _depth--;
        inst.sprite_index = category[irandom_range(0,category_max)];
        inst.spin = irandom_range(0,1);
        }
    }
