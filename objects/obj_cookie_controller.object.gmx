            <string>///increase score and delete
//https://www.reddit.com/r/gamemaker/comments/4th3lv/help_with_depth_priority_issue/
//show_message("CLICK")
audio_play_sound(snd_cookie,0,0)


var counter;
global.clicked = instance_find(obj_cookie, 0);
counter = 0
//show_message("I am "+string(global.clicked))
with(obj_cookie) {
    if((depth &lt; global.clicked.depth or counter == 0)
    and (position_meeting(mouse_x, mouse_y, self))) {
        //show_message("this object is "+string(id))
        global.clicked = id;
        counter++;
    }
}
//show_message("Clicked is "+string(global.clicked))
if(position_meeting(mouse_x, mouse_y, global.clicked)) {
    with(global.clicked) {
        if good == 1 { //good
            score += 100
            if sprite_index == spr_ewecookie 
                score += 10
            else if sprite_index == spr_owocookie
                score += 25
            with obj_score_draw alarm[0] = 1
            audio_play_sound(irandom_range(0,7),0,0)
            instance_destroy()
            }
        else if good == 0 { //bad
            score -= 500
            with obj_score_draw alarm[1] = 1
            audio_play_sound(snd_a_uhoh,0,0)
            instance_destroy()
            with obj_clock {
                clock -= 5
                alarm[1] = 1
               }
            }
        else if good == 3{ //boba
            score -= 200;
            with obj_score_draw alarm[1] = 1;
            with obj_clock {
                clock += 2.5
                alarm[0] = 1
                }  
            audio_play_sound(snd_eat,0,0);
            instance_destroy()
            }
        else if good == 4{ //banh mi
            score -= 400;
            with obj_score_draw alarm[1] = 1;
            with obj_clock {
                clock += 5
                alarm[0] = 1
            }
            audio_play_sound(snd_eat,0,0);
            instance_destroy()
        }
            
        if good == 2 { //peter
            //do not instance_destroy(), so pass
            }
    }
}
